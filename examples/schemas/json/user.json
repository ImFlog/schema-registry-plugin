{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "user",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "first_name": {
      "type": "string"
    },
    "last_name": {
      "type": "string"
    },
    "thumbnail_url": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "company": {
      "$ref": "#company"
    }
  },
  "additionalProperties": false
}
